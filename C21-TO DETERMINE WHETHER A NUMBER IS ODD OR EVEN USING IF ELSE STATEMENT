//INPUT
#include<stdio.h>
int main(){
    int n;
    printf("Enter any number: ");
    scanf("%d", n);
    if(n%2==0){
        printf("The Entered number is Even");
    }
    else{
        printf("The entered number is Odd");
    }
    return 0;
}
//OUTPUT
Enter any number: 4
The Entered number is Even
